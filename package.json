{
  "name": "napoleon-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run tests/ai",
    "test:integration": "vitest run tests/integration",
    "test:jest": "jest",
    "test:onboarding": "jest --testPathPatterns=onboarding",
    "test:auth": "jest tests/auth",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ai": "vitest run tests/ai --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "playwright:install": "playwright install",
    "quality-gate": "npm run lint && npm run type-check && npm run test:unit",
    "db:generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.ts",
    "mock-import": "tsx scripts/mock-import-emails.ts",
    "process-messages": "tsx scripts/process-ai-messages.ts",
    "install-hooks": "bash scripts/install-git-hooks.sh",
    "memory:compact": "bash scripts/memory-optimizer.sh compact",
    "memory:status": "bash scripts/memory-optimizer.sh",
    "optimize:images": "bash scripts/optimize-images.sh",
    "deploy:super": "bash scripts/deploy-and-verify.sh",
    "bundle:analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.25.4",
    "@hookform/resolvers": "^5.2.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@segment/analytics-node": "^2.1.2",
    "@sentry/nextjs": "^8.43.0",
    "@slack/web-api": "^7.9.3",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.45.1",
    "@types/canvas-confetti": "^1.9.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^3.2.37",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.19",
    "fuse.js": "^7.1.0",
    "googleapis": "^140.0.1",
    "localforage": "^1.10.0",
    "lucide-react": "^0.424.0",
    "next": "^14.2.25",
    "openai": "^4.52.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-window": "^1.8.8",
    "sharp": "^0.33.5",
    "svix": "^1.69.0",
    "tailwind-merge": "^2.4.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.25",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/localforage": "^0.0.34",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "puppeteer": "^24.15.0",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}
